type User{
    id: Int!
    createdAt:Time!
    updatedAt:Time!
    name:String!
    surname:String!
    patronymic:String!
    age:Int!
    gender:String!
    nationality:String!
}


type Query{
    users:[User!]!
    pages(page:Int!):[User!]
    }

input NewUser {
    name:String!
    surname:String!
    patronymic:String
    age:Int
    gender:String
    nationality:String
}

type Mutation {
    createUser(input:NewUser!):User!
    updateUser(UserId:Int!,input:NewUser):User!
}

scalar Time
